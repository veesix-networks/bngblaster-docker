{
  "interfaces": {
    "access": [
      {
        "interface": "eth0",
        "type": "ipoe",
        "vlan-mode": "N:1",
        "outer-vlan-min": 100,
        "outer-vlan-max": 100
      }
    ]
  },
  "sessions": {
    "count": 10,
    "max-outstanding": 10,
    "start-rate": 10,
    "stop-rate": 10
  },
  "dhcp": {
    "enable": true,
    "retry": 25
  },
  "dhcpv6": {
    "enable": true,
    "ia-na": true,
    "ia-pd": true,
    "rapid-commit": false,
    "access-line": true,
    "ldra": true
  },
  "ipoe": {
    "ipv6": true
  },
  "access-line": {
    "agent-circuit-id": "eth 0/1/2/3/{session-global}:100.{session-global}",
    "agent-remote-id": "DSLAM-{session-global}"
  }
}
